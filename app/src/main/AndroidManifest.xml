<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.jarlen.richcommon2">

    <application
        android:name=".UiApplication"
        android:allowBackup="true"
        android:icon="@drawable/cs_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".WaterMarkActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ScrollViewActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".PreviewActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".mvp.view.AddActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ViewHolderActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>